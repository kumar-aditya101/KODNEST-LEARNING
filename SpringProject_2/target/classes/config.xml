<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<!-- Setter Injection -->
<bean class= "com.learn.spring.SpringProject2.Laptop" id="lapbean1">
<property name="srNo" value="1111"> </property>
<property name="brand" value="Hp"></property>
<property name="cost" value="50000.00F"></property>
</bean>

<bean class= "com.learn.spring.SpringProject2.Laptop" id="lapbean2">
<property name="srNo" value="2222"> </property>
<property name="brand" value="Apple"></property>
<property name="cost" value="90000.54F"></property>
</bean>

<!-- Constructor Injection -->
<bean class= "com.learn.spring.SpringProject2.Laptop" id="lapbean3">
<constructor-arg value="3333"></constructor-arg>
<constructor-arg value="Dell"></constructor-arg>
<constructor-arg value="55500.45F"></constructor-arg>
</bean>
 

</beans>