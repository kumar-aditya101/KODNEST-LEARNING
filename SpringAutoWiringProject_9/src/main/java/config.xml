<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">
<context:annotation-config/>

<!--Achieving auto-wiring using annotation  -->
<bean class="com.learn.spring.SpringAutoWiringProject_8.Car" id= "cr">
</bean>

<bean class= "com.learn.spring.SpringAutoWiringProject_8.Engine" id="eng">
<property name="eng_No" value="98745"></property>
<property name="eng_Type" value="Diesel"></property>
</bean>

<bean class= "com.learn.spring.SpringAutoWiringProject_8.Engine" id="eng1">
<property name="eng_No" value="11111"></property>
<property name="eng_Type" value="Petrol"></property>
</bean>

</beans>
